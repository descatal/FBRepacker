                        <TextBlock Text="47. unknown 0xB8: " Width="105" TextAlignment="Right" />
                        <TextBox x:Name="unk_0xB8" Text="{Binding individual_Projectile_Properties.unk_0xB8, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Stretch" Margin="10,0,10,0" TextAlignment="Center" MinWidth="100"/>
                    </DockPanel>

                    <DockPanel HorizontalAlignment="Center">
                        <TextBlock Text="48. Sound Effect Hash: " Width="150" TextAlignment="Right" />
                        <TextBox x:Name="sound_effect_hash" Text="{Binding individual_Projectile_Properties.sound_effect_hash, Converter={StaticResource UIntToHexStrConverter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Stretch" Margin="10,0,10,0" TextAlignment="Center" MinWidth="100" PreviewTextInput="hexInput_PreviewTextInput"/>
                    </DockPanel>

                    <DockPanel HorizontalAlignment="Center">
                        <TextBlock Text="49. unknown 0xC0: " Width="105" TextAlignment="Right" />
                        <TextBox x:Name="unk_0xC0" Text="{Binding individual_Projectile_Properties.unk_0xC0, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Stretch" Margin="10,0,10,0" TextAlignment="Center" MinWidth="100"/>
